Algoritmo incremento_salarial
	Definir num Como Entero;
	Escribir "Su salario actual es: ";
	Leer num;
	Definir salario Como Entero;
	salario<-1200;
	Definir aumento Como Entero;
	aumento<-salario*.25;
	Escribir "Felicidades su NUevo Salario es: ",aumento;
	

	
FinAlgoritmo

